<div class="container mt-5">
  </div>
  <%= simple_form_for [ @list, @bookmark ] do |f| %>

    <div class="form-inputs">
      <%= f.input :comment %>
      <%= f.association :movie %>
    </div>

    <div class="form-actions">
      <%= f.submit "Submit Bookmark", class: "btn btn-primary" %>
    </div>
  <% end %>
</div>
